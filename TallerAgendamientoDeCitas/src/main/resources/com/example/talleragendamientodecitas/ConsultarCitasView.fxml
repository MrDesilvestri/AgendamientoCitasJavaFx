<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.talleragendamientodecitas.ConsultarCitasController" spacing="10" alignment="CENTER">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>
    <Label text="Fecha (YYYY-MM-DD):"/>
    <TextField fx:id="fechaField"/>
    <Button text="Buscar Citas" onAction="#handleBuscarCitas"/>
    <TableView fx:id="citasTable"/>
    <Button text="Volver" onAction="#handleVolver"/>
</VBox>

